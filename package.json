{
  "name": "mitmproxy-logger-action",
  "version": "1.0.0",
  "description": "GitHub Action for logging HTTP/HTTPS traffic using mitmproxy",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:main && npm run build:pre && npm run build:post && npm run build:index",
    "build:main": "ncc build src/main.js -o dist/main",
    "build:pre": "ncc build src/pre.js -o dist/pre",
    "build:post": "ncc build src/post.js -o dist/post",
    "build:index": "ncc build src/main.js -o dist",
    "test": "npm run test:standalone",
    "test:standalone": "node test/test-standalone.js"
  },
  "dependencies": {
    "@actions/artifact": "^2.3.2",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "archiver": "^7.0.1",
    "archiver-zip-encrypted": "^1.0.11"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.3"
  },
  "keywords": [
    "github-action",
    "mitmproxy",
    "http",
    "traffic",
    "logging"
  ],
  "author": "yaegashi",
  "license": "MIT"
}
